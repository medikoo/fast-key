{
  "name": "fast-key",
  "version": "0.0.0",
  "description": "Unique object key for any JavaScript value",
  "author": "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
  "keywords": [
    "id",
    "key"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/medikoo/fast-key.git"
  },
  "dependencies": {
    "es6-weak-map": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^3.18",
    "istanbul": "^0.4.5",
    "tape": "^4.6.3"
  },
  "eslintConfig": {
    "extends": "eslint:all",
    "root": true,
    "env": {
      "commonjs": true
    },
    "rules": {
      "capitalized-comments": ["error", "always", { "ignoreConsecutiveComments": true }],
      "comma-style": ["error", "last", { "exceptions": { "VariableDeclaration": true } }],
      "curly": ["error", "multi-line"],
      "dot-location": ["error", "property"],
      "func-names": ["error", "never"],
      "global-require": "off",
      "id-length": ["error", { "exceptions": ["e", "t"] } ],
      "indent": ["error", "tab"],
      "init-declarations": "off",
      "line-comment-position": "off",
      "max-len": ["error", 100],
      "max-params": "off",
      "max-statements": ["error", 20, { "ignoreTopLevelFunctions": true }],
      "multiline-ternary": "off",
      "newline-before-return": "off",
      "no-empty": ["error", { "allowEmptyCatch": true }],
      "no-extra-parens": ["error", "all", { "nestedBinaryExpressions": false }],
      "no-inline-comments": "off",
      "no-magic-numbers": ["error", { "ignore": [-1, 0, 1] }],
      "no-mixed-requires": "off",
      "no-mixed-spaces-and-tabs": ["error", "smart-tabs"],
      "no-multi-assign": "off",
      "no-multi-spaces": ["error", { "exceptions": { "VariableDeclarator": true } }],
      "no-param-reassign": "off",
      "no-plusplus": "off",
      "no-process-env": "off",
      "no-return-assign": "off",
      "no-shadow": ["error", { "builtinGlobals": true }],
      "no-tabs": "off",
      "no-ternary": "off",
      "no-undef": ["error", { "typeof": false }],
      "no-underscore-dangle": "off",
      "no-var": "off",
      "no-warning-comments": ["error", { "terms": ["fixme", "xxx"], "location": "start" }],
      "object-property-newline": ["error", { "allowMultiplePropertiesPerLine": true }],
      "object-curly-spacing": ["error", "always"],
      "one-var": "off",
      "one-var-declaration-per-line": "off",
      "padded-blocks": "off",
      "prefer-arrow-callback": "off",
      "prefer-template": "off",
      "quote-props": ["error", "consistent-as-needed"],
      "sort-keys": "off",
      "sort-vars": "off",
      "template-curly-spacing": ["error", "always"],
      "vars-on-top": "off"
    }
  },
  "scripts": {
    "lint": "eslint --ignore-path=.gitignore .",
    "test": "tape test/*.js"
  },
  "license": "MIT"
}
