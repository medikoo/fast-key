    "url": "git://github.com/medikoo/fast-key.git"
  },
  "dependencies": {
    "es6-weak-map": "^2.0.2"
  },
  "devDependencies": {
    "browserify": "^14.3",
    "browserstack-runner": "^0.5",
    "browserstack-tape-reporter": "^1.1",
    "deferred": "^0.7.6",
    "eslint": "^3.18",
    "eslint-config-medikoo-es5": "^1.1",
    "istanbul": "^0.4.5",
    "sinon": "^2.1",
    "tape": "^4.6.3"
  },
  "eslintConfig": {
    "extends": "medikoo-es5",
    "root": true
